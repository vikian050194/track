{
    "manifest_version": 3,
    "name": "track",
    "description": "Open dynamically combined URL without a single mouse click",
    "version": "1.2.0",
    "permissions": [
        "storage",
        "contextMenus"
    ],
    "background": {
        "service_worker": "background/index.js",
        "type": "module"
    },
    "action": {
        "default_icon": "icons/icon_32.png",
        "default_popup": "popup/popup.html"
    },
    "options_page": "options.html",
    "options_ui": {
        "page": "options/options.html",
        "open_in_tab": true
    },
    "commands": {
        "_execute_action": {
            "suggested_key": {
                "windows": "Alt+T",
                "mac": "Command+T",
                "chromeos": "Alt+T",
                "linux": "Alt+T"
            }
        }
    },
    "icons": {
        "16": "icons/icon_16.png",
        "32": "icons/icon_32.png",
        "128": "icons/icon_128.png"
    },
    "homepage_url": "https://github.com/vikian050194/track"
}